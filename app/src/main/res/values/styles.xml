<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#52944D</item>
        <item name="colorPrimaryDark">#52944D</item>
        <item name="colorAccent">#52944D</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#fff</item>
        <item name="colorPrimaryDark">#52944D</item>
        <item name="colorAccent">#00560F</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.DicloudIconBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#fff</item>
        <item name="colorPrimaryDark">#fff</item>
        <item name="colorAccent">#fff</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.WebActivityBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#52944D</item>
        <item name="colorPrimaryDark">#52944D</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
